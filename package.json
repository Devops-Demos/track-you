    "chai-spies": "^0.7.0",
    "mocha": "^2.2.5",
    "sinon": "^1.17.3",
    "superagent": "^2.0.0",
    "superagent-promise": "^1.1.0",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": "4.2.6"
  }
}
