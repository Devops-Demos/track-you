  "scripts": {
    "pm2:startMain": "pm2 start app.js --name $APP_NAME -l app_logs.log",
    "pm2:startJobs": "pm2 start startJobs.js --name $APP_NAME\"__jobs\" -l app_logs_jobs.log",
    "pm2:start": "npm run pm2:startMain && npm run pm2:startJobs",
    "pm2:stop": "pm2 stop $APP_NAME && pm2 delete $APP_NAME && pm2 stop $APP_NAME\"__jobs\" && pm2 delete $APP_NAME\"__jobs\"",
    "pm2:stopMain": "pm2 stop $APP_NAME && pm2 delete $APP_NAME",
    "pm2:stopJobs": "pm2 stop $APP_NAME\"__jobs\" && pm2 delete $APP_NAME\"__jobs\"",
    "pm2:restart": "npm run pm2:stop && npm run pm2:start",
    "pm2:restartMain": "npm run pm2:stopMain && npm run pm2:startMain",
    "pm2:restartJobs": "npm run pm2:stopJobs && npm run pm2:startJobs",
    "pm2": "if pm2 list | grep $APP_NAME; then npm run pm2:restart$TYPE; else npm run pm2:start$TYPE; fi",
    "start": "node app.js --environment=$ENV_NAME",
    "startJobs": "cp config/env/console.heroku.js config/local.js && node startJobs.js",
    "test": "node test-data/seed-data && mocha test/bootstrap.test test/**/*",
    "migrate": "node test-data/seed-data",
    "test:no-db": "mocha test/**/*.js",
    "subtest": "mocha test/bootstrap.test test/services/custom/recursive-aggregators/*"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
